package swaping_bitwise;
public class Swaping_bitwise {

    
    public static void main(String[] args) {
       int a=7;
       int b=14;
       
       a=a^b;
       b=a^b;
       a=a^b;
       
       System.out.println(a);
       System.out.println(b);
    }
    
}

// without using 3nd variable 
